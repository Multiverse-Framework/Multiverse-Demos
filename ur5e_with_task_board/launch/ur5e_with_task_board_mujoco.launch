<launch>

    <arg name="namespace" default="ur5e" />
    <include file="$(find ur_mujoco)/launch/ur_mujoco.launch">
        <arg name="namespace" value="$(arg namespace)" />
        <arg name="config"
            value="$(find ur5e_with_task_board)/config/ur5e_with_task_board.yaml" />
        <arg name="world" value="$(find work_table)/mjcf/work_table.xml" />
        <arg name="rviz_config" value="$(find ur5e_with_task_board)/rviz_config/rviz_config.rviz" />
    </include>

</launch>
